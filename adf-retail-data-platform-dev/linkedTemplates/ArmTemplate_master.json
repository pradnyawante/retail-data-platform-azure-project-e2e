{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-retail-data-platform-dev"
		},
		"ls_adls_customers_bronze_dev_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_customers_bronze_dev'"
		},
		"ls_adls_customers_bronze_sink_dev_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_customers_bronze_sink_dev'"
		},
		"ls_adls_orders_dev_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_orders_dev'"
		},
		"ls_adls_retail_dev_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_retail_dev'"
		},
		"ls_orders_sink_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_orders_sink'"
		},
		"ls_adls_customers_bronze_dev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretaildataplatform001.dfs.core.windows.net/"
		},
		"ls_adls_customers_bronze_sink_dev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretaildataplatform001.dfs.core.windows.net/"
		},
		"ls_adls_orders_dev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretaildataplatform001.dfs.core.windows.net/"
		},
		"ls_adls_retail_dev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretaildataplatform001.dfs.core.windows.net/"
		},
		"ls_orders_sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stretaildataplatform001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-retail-data-platform-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_customers_bronze_dev_accountKey": {
						"value": "[parameters('ls_adls_customers_bronze_dev_accountKey')]"
					},
					"ls_adls_customers_bronze_sink_dev_accountKey": {
						"value": "[parameters('ls_adls_customers_bronze_sink_dev_accountKey')]"
					},
					"ls_adls_orders_dev_accountKey": {
						"value": "[parameters('ls_adls_orders_dev_accountKey')]"
					},
					"ls_adls_retail_dev_accountKey": {
						"value": "[parameters('ls_adls_retail_dev_accountKey')]"
					},
					"ls_orders_sink_accountKey": {
						"value": "[parameters('ls_orders_sink_accountKey')]"
					},
					"ls_adls_customers_bronze_dev_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_customers_bronze_dev_properties_typeProperties_url')]"
					},
					"ls_adls_customers_bronze_sink_dev_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_customers_bronze_sink_dev_properties_typeProperties_url')]"
					},
					"ls_adls_orders_dev_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_orders_dev_properties_typeProperties_url')]"
					},
					"ls_adls_retail_dev_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_retail_dev_properties_typeProperties_url')]"
					},
					"ls_orders_sink_properties_typeProperties_url": {
						"value": "[parameters('ls_orders_sink_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}